{
  "name": "mididb",
  "productName": "mididb",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "type_": "module",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "rm:dist": "rimraf dist",
    "copy:soundfonts": "cp -r soundfont/* dist/static/",
    "build:web": "webpack --config webpack.react.config.ts --env ts-node/register --env development --mode development",
    "build:server": "npm run build:web && tsc -p src/node/ --outDir dist && npm run copy:soundfonts",
    "start:server": "npm run build:server && node dist/node/server.js"
  },
  "keywords": [],
